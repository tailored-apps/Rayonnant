<view:PrismViewBase x:Class="TailoredApps.Rayonnant.DummyModule.View.OtherContentView"
                    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:view="clr-namespace:TailoredApps.Rayonnant.Presentation.View;assembly=TailoredApps.Rayonnant.Presentation"
                    xmlns:regions="http://www.codeplex.com/CompositeWPF"
                    mc:Ignorable="d"
                    Style="{StaticResource {x:Type view:PrismViewBase}}"
                    d:DesignHeight="300" d:DesignWidth="300">

    <Grid Margin="0,0,-270,0" RenderTransformOrigin="0.526,0.507">
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="10*" />
            <ColumnDefinition Width="9*" />
        </Grid.ColumnDefinitions>
        <Label Grid.Row="0" Content="OtherContentView" />

        <Button Grid.Row="1" Command="{Binding Button}" />
    </Grid>
</view:PrismViewBase>