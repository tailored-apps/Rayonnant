<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:TailoredApps.Rayonnant.Presentation.View"
                    xmlns:viewModel="clr-namespace:TailoredApps.Rayonnant.Presentation.ViewModel"
                    xmlns:viewModel1="clr-namespace:TailoredApps.Rayonnant.Presentation.Interface.ViewModel">

    <DataTemplate DataType="{x:Type viewModel:ShellViewModel}">
        <view:ShellView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:StatusViewModel}">
        <view:StatusView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:CommandsViewModel}">
        <view:CommandsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:MenuViewModel}">
        <view:MenuView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModel:OpenItemsViewModel}">
        <view:OpenItemsView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModel:UserPreferencesSettingsViewModel}">
        <view:UserPreferencesSettingsView/>
    </DataTemplate>
</ResourceDictionary>